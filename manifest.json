{
  "manifest_version": 2,
  "version": "0.2",
  "name": "Speed Dial for GMail",

  "permissions": ["speeddial", "https://mail.google.com/"],
  "background": {"page": "speeddial.html"},
  "speeddial": {
    "title": "GMail",
    "url": "https://mail.google.com"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  }
}